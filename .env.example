# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# Gemini API Key (should be handled server-side in production)
GEMINI_API_KEY=your_gemini_api_key_here

# Convex Configuration
# Get these values from your Convex dashboard: https://dashboard.convex.dev
NEXT_PUBLIC_CONVEX_URL=your_convex_url_here
CONVEX_DEPLOYMENT=your_convex_deployment_name_here

# Payments
POLAR_ACCESS_TOKEN=
POLAR_SUCCESS_URL=/success?checkout_id={CHECKOUT_ID}
POLAR_WEBHOOK_SECRET=

# Subscriptions
NEXT_PUBLIC_STARTER_TIER=
NEXT_PUBLIC_STARTER_SLUG=
