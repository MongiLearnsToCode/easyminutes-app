# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# Gemini API Key (should be handled server-side in production)
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Cloud Configuration
# Get these values from your Supabase Cloud dashboard: https://supabase.com/dashboard
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Production API Endpoint (when using server-side proxy)
VITE_API_ENDPOINT=https://your-api-server.com/api

# Lemon Squeezy Configuration
VITE_LEMONSQUEEZY_API_KEY=your_lemonsqueezy_api_key_here
VITE_LEMONSQUEEZY_STORE_ID=your_lemonsqueezy_store_id_here
VITE_LEMONSQUEEZY_VARIANT_ID=your_lemonsqueezy_variant_id_here
LEMONSQUEEZY_SIGNING_SECRET=your_signing_secret_here

